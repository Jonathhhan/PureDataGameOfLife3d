\; local pix = ofPixels() \; a.volumetrics = ofVolumetrics() \; local
imageSequence = ofImageSequencePlayer() \; a.volWidth \, a.volHeight
\, a.volDepth \, counter = 0 \, 0 \, 0 \, -1 \; a.fboCells = ofFbo()
\; a.shaderGameOfLife = ofShader() \; cam = ofEasyCam() \; \; function
a.new() \; ofWindow.addListener("setup" \, this) \; ofWindow.addListener("update"
